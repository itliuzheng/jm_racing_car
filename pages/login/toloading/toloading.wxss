/* pages/login/login.wxss */
page{
  background: #f6f6f6;
}
input,button{
  box-sizing: border-box;
}
.login-bj{
  width: 100%;
  min-height: 560rpx;
  text-align: center;
  position: relative;
  padding-top: 150rpx;
}
.login-bj .bj{
  width: 100%;
  height: 560rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.login-content{
  position: relative;
  z-index: 2;
  padding: 0 28rpx;
}

.login-bj .logo{
  width: 493rpx;
  height: 174rpx;
}

.login-form{
  width: 100%;
  background: #fff;
  border-radius: 10rpx;
  margin-top: 28rpx;
  padding: 120rpx 0 140rpx;
  position: relative;
}

.btn-area{
  margin-top: 78rpx;
}
.btn-area button{
  width: 606rpx;
  border-radius: 50rpx;
  line-height: 1;
  background: #fa4040;
  color: #fff;
  font-size: 30rpx;
  padding: 34rpx 0;
  margin-bottom: 10rpx;
}