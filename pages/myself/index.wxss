/**index.wxss**/
.myself{
  height: 100%;
  background: #fafafa;
}
.userinfo{
  width: 100%;
  height: 324rpx;
  border-bottom: 2rpx solid #d8dfe7;
  padding: 87rpx 28rpx 0;
  background: #fff;
  position: relative;
}

.userinfo-avatar {
  width: 104rpx;
  height: 104rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  display: inline-block;
  color: #000;
  font-size: 46rpx;
  line-height: 1;
  margin-left: 26rpx;
  vertical-align: top;
}

.userinfo-nickname .nickname{
  display: block;
  margin: 13rpx 0 15rpx;
  line-height: 1;
}
.userinfo-nickname .nickname_desc{
  display: block;
  font-size: 20rpx;
  color: #b7b7b7;
  font-weight: normal;
  line-height: 1;
}

.veservation_amount{
  display: inline-block;
  margin-top: 20rpx;
  padding: 0 14rpx 0 20rpx;
  height: 48rpx;
  background: #ffefef;
  color: #fc5d5d;
  font-weight: bold;
  line-height: 48rpx;
  font-size: 20rpx;
}

.myself-bg{
  position: absolute;
  right: 0;
  bottom: 0;
  width: 245rpx;
  height: 324rpx;
}

.my-info{
  padding: 0 28rpx 15rpx; 
}

.my-info .my-info-title{
  line-height: 1;
  padding: 44rpx 0;
  font-size: 34rpx;
  color: #000;
  font-weight: 500;
}

.list-cell{
  height: 141rpx;
  line-height: 141rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 30rpx; 
  box-shadow: 1rpx 17rpx 20rpx 0px rgba(233, 234, 235, 0.59);
  
  position: relative;
}
.list-cell.list-cell-link:before{
  content: "";
  position: absolute;
  right: 30rpx;
  top: 0;
  width: 11rpx;
  height: 100%;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% 19rpx;
}
.list-cell .list-icon{
  width: 76rpx;
  height: 76rpx; 
  margin-top: 34rpx;
  margin-right: 33rpx;
  margin-left: 32rpx;
}
.list-cell text{
  font-size: 34rpx;
}