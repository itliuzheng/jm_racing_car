page{
  background: #FBFBFD;
}
.list_box{
  padding: 0 28rpx;
}
.list{
  width: 100%;
  min-height: 190rpx;
  background: #fff;
  margin-top: 28rpx;
  padding: 35rpx 0 35rpx 32rpx;
  position: relative;
}
.list:after{
  content: '';
  position: absolute;
  right: 17rpx;
  top: 0;
  width: 11rpx;
  height: 100%;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 100% 21rpx;
  background-position: 0 center;
}
.p{
  font-size:24rpx;
  font-weight:500;
  color:rgba(131,131,131,1);
  line-height: 1;
  margin-bottom: 26rpx;
  overflow-y: hidden;
}
.p:last-child{
  margin-bottom: 0;
}