.header{
  width: 100%;
  height: 630rpx;
  background: #EEF8FF;
}
.time{
  display: block;
  width: 100%;
  font-size:26rpx;
  font-weight:500;
  color:rgba(131,131,131,1);
  text-align: center;
  margin: 50rpx 0;
}
.list_box{
  padding: 0 0 0 22rpx;
}
.list{
  width:215rpx;
  height:147rpx;
  border:1rpx solid rgba(68,187,255,1);
  border-radius:7rpx;
  float: left;
  padding-top: 35rpx;
  margin-right: 25rpx;
  margin-bottom: 22rpx;
}
.title,.desc{
  font-size:31rpx;
  font-weight:500;
  color:rgba(68,187,255,1);
  text-align: center;
  line-height: 1;
  overflow-y: hidden;
}
.desc{
  margin-top: 28rpx;
}