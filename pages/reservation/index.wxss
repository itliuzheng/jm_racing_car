.stv-container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.stv-container .tab-bar {
  position: relative;
  display: flex;
  font-size: 28rpx;
  height: 98rpx;
  color: #000;
}
.stv-container .tab-bar::before{
  content: '';
  position: absolute;
  width: 1rpx;
  height: 24rpx;
  background: #dedede;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
 .stv-container .tab-bar .tab-active text{
  /* color: #2d80ff; */
  position: relative;
} 
.stv-container .tab-bar .tab-active text::before{
  content: '';
  width: 100%;
  height: 5rpx;
  background: #f85a5e;
  position: absolute;
  bottom: -8rpx;
  left: 0;
} 

.stv-container .tab-bar .tab {
  width: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 16rpx;
  padding-bottom: 20rpx;
}
.one-scene{
  background: #f0f0f5;
  padding: 0 28rpx 98rpx;
}
.swiper{
   background: #f0f0f5;
}

/* 时间滑动 */
.scroll-x{
    height:80px;
    white-space:nowrap;
    display:flex;
}
/* 隐藏scrollbar */
::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
}

.select-header .select-time{
  display:inline-block;
  margin-bottom: 40rpx;
}
.select-time .time-list{
  margin: 0 13rpx;
}
.time-list{
  text-align: center;
  color: #6f788a;
  background: #e3e3e9;
  border-radius: 4rpx;
  padding-top: 14rpx;
  padding-bottom: 17rpx;
  border: 1rpx solid transparent;
}
.time-list.active{
  background-image: linear-gradient(180deg, #fe6b45,#d61b35);
  color: #fff;
}
.time-list.now{
  border:1rpx solid #bbbbcf;
}
.time-list .month{
  font-size: 12rpx;
  line-height: 1;
  margin-bottom: 4rpx;
}
.time-list .week{
  font-size: 24rpx;
  line-height: 1;
  margin-bottom: 5rpx;
}
.time-list .day{
  font-size: 41rpx;
  line-height: 1;
}


/* end */


/* 选择想要的场地/时间 */
.select-header .select-place{
  padding-top: 60rpx;
}
.select-place .picker{
  position: relative;
  padding-left: 35rpx;
  height: 30rpx;
  line-height: 30rpx;
  margin-bottom: 35rpx;
}
.select-place .picker::before{
  content: "";
  position: absolute;
  left: 0;
  top: 0;
  width: 25rpx;
  height: 30rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
}
.select-place .picke text{
  font-size: 28rpx;
}

